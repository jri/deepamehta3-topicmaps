{
    topic_types: [
        {
            label: "Topicmap",
            uri: "de/deepamehta/core/topictype/Topicmap",
            icon_src: "/de.deepamehta.3-topicmaps/images/network.png",
            js_renderer_class: "PlainDocument",
            fields: [
                {
                    label: "Title",
                    uri: "de/deepamehta/core/property/Title",
                    data_type: "text",
                    editor: "single line",
                    js_renderer_class: "TitleRenderer",
                    indexing_mode: "FULLTEXT"
                },
                {
                    label: "Description",
                    uri: "de/deepamehta/core/property/Description",
                    data_type: "html",
                    js_renderer_class: "BodyTextRenderer",
                    indexing_mode: "FULLTEXT"
                }
            ]
        },
        {
            label: "Topicmap Relation Ref",
            uri: "de/deepamehta/core/topictype/TopicmapRelationRef",
            js_renderer_class: "PlainDocument",
            fields: [
                {
                    label: "Relation ID",
                    uri: "de/deepamehta/core/property/RelationID",
                    data_type: "number",
                    indexing_mode: "KEY"
                }
            ]
        }
    ]
}
